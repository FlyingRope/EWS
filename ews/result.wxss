#panel{
  height:65vh;
  background:#fff;
}
#panel text{
  line-height: 18vh;
  border-right: 1rpx solid #ddd;
  border-top: 1rpx solid #ddd;
  border-left: 1rpx solid #ddd;
  border-bottom: 1rpx solid #ddd;
}
#panel image{
  height: 18vh;
  border-right: 1rpx solid #ddd;
  border-top: 1rpx solid #ddd;
  border-left: 1rpx solid #ddd;
  border-bottom: 1rpx solid #ddd;
}
.flex-row{
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
}
.flex-column{
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: stretch;
}
.flex-cell{
  flex: 0 0 30%;
}
.flex-cell2{
  flex: 0 0 50%;
}
