.image-style {
  position: absolute;
  bottom: 240px;
  right: 100px;
  width: 60px;
  height: 60px;
  z-index: 100;
}

.ball {
  box-shadow: 2px 2px 10px #aaa;
  border-radius: 20px;
  position: absolute;
}

.table {
  border: 1px solid #f8f8f8;
  border-right: 0;
  border-bottom: 0;
  width: 100%;
}

.tr {
  width: 100%;
  display: flex;
  justify-content: space-between;
}

.th, .td {
  padding: 10px;
  border-bottom: 1px solid #f8f8f8;
  border-right: 1px solid #f8f8f8;
  text-align: center;
  width: 100%;
  font-size: 30px;
}

.th {
  font-weight: 400;
  background-color: #f8f8f8;
}

.page {
  background-image: url("http://ac-f5oadpis.clouddn.com/91f3499f54ad1979573d.png");
  background-size:cover;
}

.mid-text {
  width: 100%;
  height: 100%;
  display: flex;
  justify-content: center;
}

.start_button {
  display: inline-block;
  padding: 15px 25px;
  font-size: 24px;
  cursor: pointer;
  text-align: center;
  text-decoration: none;
  outline: none;
  color: #fff;
  background-color: #4caf50;
  border: none;
  border-radius: 15px;
  box-shadow: 0 9px #999;
  position: absolute;
  left: 0;
  right: 0;
}

.start_button:hover {
  background-color: #3e8e41;
}

.start_button:active {
  background-color: #3e8e41;
  box-shadow: 0 5px #666;
  transform: translateY(4px);
}
